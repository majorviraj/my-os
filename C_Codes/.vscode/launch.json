    "version": "0.2.0",
    "configurations": [	
		
		{
			"type": "gdb",
			"request": "attach",
			"name": "Attach to gdbserver",
			"executable": "${workspaceFolder}/cDerivationOfThese/jtag/build/output.elf",
			"target": ":3333",
			"remote": true,
			"cwd": "${workspaceRoot}"
		},
		{
			"name": "(gdb) Launch Real",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/cDerivationOfThese/Emmc/build/output.elf",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"miDebuggerPath": "arm-none-eabi-gdb",
			"logging": { "engineLogging": true },
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"text": "set endian little",
					"ignoreFailures": false
				},
				{
					"text": "target remote :3333",
					"ignoreFailures": false
				},
				// {
				// 	"text": "file ${workspaceFolder}/cDerivationOfThese/Emmc/build/output.elf",
				// 	"ignoreFailures": false
				// }
				
			]
		}
    ]
}